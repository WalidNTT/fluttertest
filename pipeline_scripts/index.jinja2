<!DOCTYPE html>
<html>
<head>
  <meta content="width=device-width" name="viewport" />
    <title>Flutter Test</title>
    <style>
    body { text-align:center }
    a { color: firebrick; text-decoration:none }
    </style>
  <link rel="stylesheet" href="html.css">

</head>
<body>

<div class="wrapper">
    <img class="search-icon" src="data:image/svg+xml;utf8;base64," />
    <input id="search" class="search" placeholder="Search for branch..." type="text" onkeyup="myFunction()">
</div>  

<div class="row">
    {% for row in context_data %}
    <div class="column">
        <div class="card">
            <img src={{ row[9] }} alt="Avatar" style="width:100%"> 
            <div class="container">
            <h4>
            {% if 'Android' in row[3] %}
                <a href={{ row[2] }}/{{ row[1] }}?access_token={{ row[6] }}>
                    <p style="display:inline" style="font-size:10px">&nbsp;&nbsp;{{ row[0] }}</p>
                </a>
            {% elif 'iOS' in row[3] %}
                <a href={{ row[2] }}%2F{{ row[1] }}%3Faccess_token%3D{{ row[6] }}>
                    <p style="display:inline" style="font-size:10px">&nbsp;&nbsp;{{ row[0] }}</p>
                </a>
            {% endif %}
            </h4>
            <p>{{ row[4]}}</p>
            {% if 'Android' in row[3] %}
            <p><a href={{ row[7] }}><p style="font-size:15px"><img src=./android.png style="width: 10px;">&nbsp;{{ row[8] }}</p></a></p>
            {% elif 'iOS' in row[3] %}
            <p><a href={{ row[7] }}><p style="font-size:15px"><img src=./ios.png style="width: 10px;">&nbsp;{{ row[8] }}</p></a></p>
            {% endif %}
            </div>
        </div>
    </div>
    {% endfor %}
</div>

<script>
    function myFunction() {
        var input = document.getElementById("search");
        var filter = input.value.toLowerCase();
        var nodes = document.getElementsByClassName('column');
      
        for (i = 0; i < nodes.length; i++) {
          if (nodes[i].innerText.toLowerCase().includes(filter)) {
            nodes[i].style.display = "block";
          } else {
            nodes[i].style.display = "none";
          }
        }
      }
</script>

</body>
</html>
